names:
    fastcdr:
        ctest-args: [
            "--repeat", "until-pass:3",
            "--timeout", "300",
            "--output-junit", "junit/junit.xml"
        ]
    googletest-distribution:
        cmake-args:
            - "-Dgtest_force_shared_crt=ON"
            - "-DBUILD_SHARED_LIBS=ON"
            - "-DBUILD_GMOCK=ON"
